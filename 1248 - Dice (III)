#include<bits/stdc++.h>
using  namespace std;
#define fast {ios_base::sync_with_stdio(false); cin.tie(0);}
typedef long long ll;

int main()
{
    fast
    ll i,j,k,m,n,c,t,x,y,cs=1;
    cin>>t;
    while(t--){
        cin>>n;
        double ans=0.0;
        for(i=1;i<=n;i++){
            ans+=(n+0.0)/(n-i+1);
        }
        cout<<"Case "<<cs++<<": "<<fixed<<setprecision(10)<<ans<<"\n";
    }
}

